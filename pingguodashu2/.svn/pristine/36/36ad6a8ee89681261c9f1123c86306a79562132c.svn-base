.sailfishSlider{
	position: relative;
}

.sailfishSlider .slide{
	position: absolute;
	width: 100%;
	height: 100%;
	-webkit-transform: translate3d(0, 0, 0);
}

.locator{
	position: absolute; 
	bottom: 0; left: 0; 
	width: 100%; 
	height: 6px;
	font-size: 0;
}
.locator .locator-item{
	display: inline-block; 
	height: 100%; width: 33.33%;
	background-color: #ccc;
	-webkit-transition: all 0.5s cubic-bezier(0.99, 0, 0.66, 1);
}
.locator .locator-item.active{
	background-color: #a55;
}