*{outline:none!important;-moz-outline:none!important;font-family:georgia,serif}body{overflow-x:hidden;margin-top:-9px;font-size:18px}blockquote p{font-size:inherit}body{max-width:900px;margin:0 auto}#content{max-width:700px;margin:0 auto}time{display:block;margin-bottom:5px}#top-header{margin-top:20px;margin-bottom:60px}#top-header ul{text-align:center;margin-top:10px;padding-bottom:18px;list-style-type:none;border-bottom:1px solid rgba(136,136,136,0.4)}#top-header ul li{display:inline}#top-header ul li a{color:#888}#top-header li.active,#top-header li a:hover{border-bottom:1px solid #d00}#top-header h1{margin-top:70px;line-height:1em;text-align:center;font-size:66px}#top-header a{color:inherit;text-decoration:none;border:0}article p,.article p{margin-bottom:15px}article ul,.article ul,article ol,.article ol{margin:0;margin-bottom:25px;padding-left:40px}article code,.article code,article pre,.article pre{font-size:15px}article p,.article p,article li,.article li,article blockquote,.article blockquote{line-height:28px;margin:0}a:not(.img){color:#a00;text-decoration:none;border-bottom:1px dotted #b00}a:not(.img):hover,a:not(.img):focus{text-decoration:none;border-bottom:1px solid #d00;color:#d00}article,.article{padding-bottom:40px;margin-bottom:45px;border-bottom:1px solid rgba(136,136,136,0.4)}article h3,.article h3{font-size:42px;margin-bottom:25px;text-align:center;line-height:50px}article .meta,.article .meta{text-align:center;margin-bottom:30px;color:#888}#home-footer{text-align:center}#home-footer h2{font-size:24px;margin-bottom:40px;line-height:24px}#home-footer .author{color:#888;font-size:14px}#article .article{border:0;padding-bottom:0}#archives h3 a{color:inherit;border:0}#archives .read-more{text-align:center;margin-top:20px}#archives .classes{margin-left:38%;min-height:300px}#archives .classes ul{list-style-type:none}